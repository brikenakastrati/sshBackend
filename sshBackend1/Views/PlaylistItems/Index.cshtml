@model IEnumerable<sshBackend1.Models.PlaylistItem>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GenreId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Length)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MusicProvider)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TenantId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GenreId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Length)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MusicProvider.MusicProviderId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenantId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PlaylistItemId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PlaylistItemId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PlaylistItemId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
