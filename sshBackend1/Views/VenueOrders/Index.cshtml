                @Html.DisplayFor(modelItem => item.Event.EventId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderStatus.OrderStatusId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Venue.VenueId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenantId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VenueOrderId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VenueOrderId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VenueOrderId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
